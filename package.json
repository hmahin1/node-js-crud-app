{
  "name": "autoleap-backend",
  "version": "1.0.0",
  "description": "auto leap backend api",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "start": "nodemon ./src/index.js --exec babel-node --presets env set NODE_TLS_REJECT_UNAUTHORIZED =0",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "dev": "set NODE_TLS_REJECT_UNAUTHORIZED =0&& set NODE_ENV=development&& npm start",
    "prod": "set NODE_TLS_REJECT_UNAUTHORIZED =0&& set NODE_ENV=production&& npm start",
    "build:dev": "set NODE_ENV=development&&npm run build",
    "build:prod": "set NODE_ENV=production&&npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
