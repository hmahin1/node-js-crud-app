  "homepage": "",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-watch": "^2.0.7",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "express": "^4.16.3",
    "express-validator": "^5.3.1",
    "http": "0.0.1-security",
    "mysql2": "^1.7.0",
    "node-input-validator": "^4.2.1",
    "node-schedule": "^1.3.0",
    "nodemailer": "^5.0.0",
    "nodemon": "^1.17.5",
    "sequelize": "^4.37.10",
    "sequelize-cli": "^4.0.0",
